generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String   @unique
  password  String
  role      Role     @default(VOLUNTEER)
  createdAt DateTime @default(now())
}

enum Role {
  VOLUNTEER
  ADMIN
  ORGANIZATION
}
